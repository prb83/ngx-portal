{
  "index": "/index.html",
  "assetGroups": [{
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    },
    {
      "name": "fonts",
      "resources": {
        "urls": [
          "https://fonts.googleapis.com/**",
          "https://fonts.gstatic.com/**",
          "https://use.fontawesome.com/releases/v5.7.0/css/**"
        ]
      }
    },
    {
      "name": "external",
      "resources": {
        "urls": [
          "https://use.fontawesome.com/releases/v5.7.0/css/**",
          "https://fonts.googleapis.com/**",
          "https://fonts.gstatic.com/**"
        ]
      }
    }
  ],
  "dataGroups": [{
    "name": "s3-data",
    "urls": ["https://s3.eu-central-1.amazonaws.com/nangex-assets/media/pictures/**"],
    "cacheConfig": {
      "strategy": "freshness",
      "timeout": "3s",
      "maxAge": "3d",
      "maxSize": 10
    }
  }]
}
